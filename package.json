{
  "name": "doc-genie",
  "version": "0.1.0",
  "description": "AI-powered documentation enforcement tool that automatically generates JSDoc comments for new functions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "bin": {
    "doc-genie": "./dist/cli.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bilalanees98/docgenie.git"
  },
  "bugs": {
    "url": "https://github.com/bilalanees98/docgenie/issues"
  },
  "homepage": "https://github.com/bilalanees98/docgenie#readme",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "version": "npm run format && git add -A src",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "documentation",
    "jsdoc",
    "ai",
    "openai",
    "git",
    "automation",
    "typescript",
    "code-quality"
  ],
  "author": "Muhammad Bilal",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "diff": "^7.0.0",
    "dotenv": "^16.4.7",
    "inquirer": "^12.5.0",
    "openai": "^4.28.0",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/diff": "^7.0.1",
    "@types/estree": "^1.0.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@typescript-eslint/types": "^7.1.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.1.2",
    "typescript": "~5.5.0"
  }
}
